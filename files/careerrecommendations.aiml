<category>
  <pattern>RECOMMEND</pattern>
  <template>
    <condition name="q1">
      <li value="1">Result: Software Engineer</li>
      <li value="2">Result: Data Analyst/Scientist</li>
      <li value="3">Result: Cybersecurity</li>
      <li value="4">Result: UX Designer</li>
      <li value="5">Result: Cloud/DevOps</li>
      <li>Try START.</li>
    </condition>
    Type START to try again.
  </template>
</category>

<category>
  <pattern>*</pattern>
  <template>
    <condition name="stage">
      <li value="q1">Enter 1-5 for Q1.</li>
      <li value="q2">Enter 1-5 for Q2.</li>
      <li value="q3">Enter 1-3 for Q3.</li>
      <li>Type START to begin.</li>
    </condition>
  </template>
